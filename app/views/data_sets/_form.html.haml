= form_for @data_set do |f|
  - if @data_set.errors.any?
    #error_explanation
      %h2= "#{pluralize(@data_set.errors.count, "error")} prohibited this data_set from being saved:"
      %ul
        - @data_set.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :title
    = f.text_field :title
  .field
    = f.label :description
    = f.text_area :description
  .field
    = f.label :createdate
    = f.date_select :createdate
  .actions
    = f.submit 'Save'
